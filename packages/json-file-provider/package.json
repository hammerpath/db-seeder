{
    "name": "db-seeder-json-file",
    "version": "0.1.0",
    "description": "Seed a json file with data",
    "keywords": ["json", "json-server", "seed", "database", "testing", "e2e", "integration", "playwright", "cypress"],
    "license": "ISC",
    "author": "hammerpath",
    "homepage": "https://github.com/hammerpath/db-seeder/tree/master/packages/db-seeder-json-file",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hammerpath/db-seeder.git"
    },
    "main": "./dist/index.js",
    "bin": {
        "db-seeder": "dist/bin/cli.js"
    },
    "scripts": {
        "lint": "tsc",
        "build:cli": "esbuild src/bin/cli.ts --bundle --platform=node --outfile=dist/bin/cli.js --format=esm",
        "build:server": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js",
        "build": "run-p build:*",
        "postbuild": "chmod +x dist/bin/cli.js",
        "test": "./src/repositories/__tests__/test.ps1",
        "dev:tsc": "tsc --watch --preserveWatchOutput",
        "dev:node": "node --watch dist/index.js",
        "dev:esbuild": "npm run build --watch",
        "dev": "run-p dev:*"
    },
    "dependencies": {
        "dotenv": "^17.2.1",
        "edit-json-file": "^1.8.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.32.0",
        "@types/edit-json-file": "^1.7.3",
        "esbuild": "^0.25.8",
        "eslint": "^9.32.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.6.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.38.0"
    }
}
